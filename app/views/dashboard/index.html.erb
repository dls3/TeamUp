
<h1>Dashboard</h1>

<div id="map"></div>

<script>
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: 43.642, lng: -79.387},
      zoom: 8
    });
  }
</script>


