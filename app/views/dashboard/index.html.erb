<!-- views of all games -->

<h2 class = 'current_games'> Current Games </h2>

  <% @games 3.times do |game| %>
   <p>
    <a href=/games/<%=game.id %>><%=game.name%></a> <%= game.sport %></a>
  </p>
 <% end %>

