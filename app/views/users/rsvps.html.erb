<%= link_to 'Back to Profile', users_profile_path, class: 'back_profile' %>

<h1>Your Joined Games</h1>
<% @rsvps.each do |rsvp| %>
  <ul>
    <li>
      <% m = Game.find_by(id:rsvp.game_id) %>
      <p> Event Name: <%= m.event_name %> </p>
      <p> Sport: <%= m.sport %> </p>
      <p> Players Needed: <%= m.players_needed %> </p>
      <p> Current Players: <%= m.current_players %> </p>
      <p> Skill: <%= m.skill %> </p>
      <p> Time: <%= m.time %> </p>
    </li>
  </ul>
<% end %>
